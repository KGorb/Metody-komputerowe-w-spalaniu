FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue p 101325
);

regions
(
    boxToCell
    {
        box (0 0 -1) (0.0589 0.065 1);
        fieldValues
        (
            volScalarFieldValue p 1013250
        );
    }
);


